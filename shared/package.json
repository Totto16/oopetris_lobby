{
  "name": "oopetris-lobby-shared",
  "version": "0.0.1",
  "description": "The oopetris lobby shared types",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "node": "22.16.0"
  },
  "scripts": {
    "install": "tsc"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./user": {
      "import": "./src/user/index.ts",
      "default": "./dist/user/index.js",
      "types": "./dist/user/index.d.ts"
    },
    "./user/constants": {
      "import": "./src/user/constants.ts",
      "default": "./dist/user/constants.js",
      "types": "./dist/user/constants.d.ts"
    },
    "./lobby": {
      "import": "./src/lobby/index.ts",
      "default": "./dist/lobby/index.js",
      "types": "./dist/lobby/index.d.ts"
    },
    "./general": {
      "import": "./src/general.ts",
      "default": "./dist/general.js",
      "types": "./dist/general.d.ts"
    }
  },
  "dependencies": {
    "typescript": "^5.6.3"
  }
}
